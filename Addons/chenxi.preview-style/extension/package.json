{
  "name": "preview-style",
  "publisher": "chenxi",
  "displayName": "Preview Style",
  "description": "预览class类名的css样式",
  "version": "0.0.1",
  "icon": "image/icon.png",
  "engines": {
    "vscode": "^1.1.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "styles": [
        {
            "stylesheet": "./styles.css"
        }
    ],
    "commands": [
      {
        "command": "preview-style.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.1.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
